{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },

  "env": {
    "es6": true,
    "node": true
  },

  "plugins": [],

  "globals": {
    "document": false,
    "navigator": false,
    "window": false
  },

  "rules": {
    "object-curly-spacing": ["error", "always"]
  }
}